{
  "name": "graphql-loader-example",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "codegen": "graphql-codegen --config src/codegen.ts",
    "lint": "biome lint",
    "format": "biome format",
    "format:write": "biome format --write"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "express": "^5.1.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^6.0.6",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@eddeee888/gcg-typescript-resolver-files": "^0.12.2",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "graphql-scalars": "^1.24.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
